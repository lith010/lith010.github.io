<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<extension>
    <id>org.apache.commons:commons-digester3</id>
    <version>3.2</version>
    <type>jar</type>
    <name>Apache Commons Digester</name>
    <summary>The Apache Commons Digester package lets you configure an XML to Java
    object mapping module which triggers certain actions called rules whenever
    a particular pattern of nested XML elements is recognized.</summary>
    <website>http://commons.apache.org/digester/</website>
    <authors>
        <author>
            <name>Craig McClanahan</name>
        </author>
        <author>
            <name>Robert Burrell Donkin</name>
        </author>

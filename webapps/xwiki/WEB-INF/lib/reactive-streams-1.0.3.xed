        </repository>
        <repository>
            <id>xwiki-public</id>
            <type>maven</type>
        </repository>
    </repositories>
    <properties>
        <maven.groupid>org.reactivestreams</maven.groupid>
        <maven.artifactid>reactive-streams</maven.artifactid>
    </properties>
</extension>

<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<extension>
    <id>org.reactivestreams:reactive-streams</id>
    <version>1.0.3</version>
    <type>jar</type>
    <name>reactive-streams</name>
    <summary>A Protocol for Asynchronous Non-Blocking Data Sequence</summary>
    <website>http://www.reactive-streams.org/</website>
    <authors>
        <author>
            <name>Reactive Streams SIG</name>
            <url>http://www.reactive-streams.org/</url>
        </author>
    </authors>
    <licenses>
        <license>
            <name>CC0</name>
        </license>
    </licenses>
    <scm>
        <url>git@github.com:reactive-streams/reactive-streams.git</url>
        <connection>
            <system>git</system>
            <path>git@github.com:reactive-streams/reactive-streams.git</path>
        </connection>
    </scm>
    <repositories>
        <repository>
            <id>xwiki-snapshots</id>

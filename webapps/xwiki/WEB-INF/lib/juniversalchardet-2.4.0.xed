<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<extension>
    <id>com.github.albfernandez:juniversalchardet</id>
    <version>2.4.0</version>
    <type>jar</type>
    <name>juniversalchardet</name>
    <summary>JUniversalChardet is a Java encoding detector library</summary>
    <website>https://github.com/albfernandez/juniversalchardet</website>
    <authors>
        <author>
            <name>Alberto Fern√°ndez</name>
        </author>
    </authors>
    <licenses>
        <license>
            <name>Mozilla Public License Version 1.1</name>
        </license>
        <license>
            <name>GENERAL PUBLIC LICENSE, version 3 (GPL-3.0)</name>
        </license>
        <license>
            <name>GNU LESSER GENERAL PUBLIC LICENSE, version 3 (LGPL-3.0)</name>
        </license>
    </licenses>
    <scm>
        <url>git@github.com:albfernandez/juniversalchardet.git</url>
        <connection>
            <system>git</system>
            <path>git@github.com:albfernandez/juniversalchardet.git</path>

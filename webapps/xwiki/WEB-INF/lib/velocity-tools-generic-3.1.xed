        <connection>
            <system>git</system>
            <path>https://gitbox.apache.org/repos/asf/velocity-tools.git/velocity-tools-generic</path>
        </connection>
        <developerconnection>
            <system>git</system>
            <path>https://gitbox.apache.org/repos/asf/velocity-tools.git/velocity-tools-generic</path>
        </developerconnection>
    </scm>
    <issuemanagement>
        <system>jira</system>
        <url>https://issues.apache.org/jira/browse/VELTOOLS</url>
    </issuemanagement>
    <dependencies>
        <dependency>
            <id>org.apache.velocity:velocity-engine-core</id>
            <version>2.3</version>
            <optional type="boolean">false</optional>
            <properties>
                <maven.dependency.scope>compile</maven.dependency.scope>
                <maven.dependency.type>jar</maven.dependency.type>
            </properties>
        </dependency>
        <dependency>
            <id>commons-beanutils:commons-beanutils</id>
            <version>1.9.4</version>
            <optional type="boolean">false</optional>
            <properties>
                <maven.dependency.scope>compile</maven.dependency.scope>
                <maven.dependency.type>jar</maven.dependency.type>
            </properties>
        </dependency>
        <dependency>
            <id>org.apache.commons:commons-digester3</id>
            <version>3.2</version>
            <optional type="boolean">false</optional>
            <properties>
                <maven.dependency.scope>compile</maven.dependency.scope>
                <maven.dependency.type>jar</maven.dependency.type>
            </properties>
            <exclusions>
                <extensionpattern>
                    <id>cglib:cglib</id>
                </extensionpattern>
            </exclusions>
        </dependency>
        <dependency>
            <id>org.apache.commons:commons-lang3</id>
            <version>3.10</version>
            <optional type="boolean">false</optional>

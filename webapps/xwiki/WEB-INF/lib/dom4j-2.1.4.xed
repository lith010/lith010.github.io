<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<extension>
    <id>org.dom4j:dom4j</id>
    <version>2.1.4</version>
    <type>jar</type>
    <name>dom4j</name>
    <summary>flexible XML framework for Java</summary>
    <website>http://dom4j.github.io/</website>
    <extensionfeatures>
        <feature>
            <feature>dom4j:dom4j</feature>
            <version>2.1.4</version>
        </feature>
    </extensionfeatures>
    <features>
        <feature>dom4j:dom4j</feature>
    </features>
    <authors>
        <author>
            <name>Filip Jirs√°k</name>
            <url>https://github.com/FilipJirsak</url>
        </author>
    </authors>
    <licenses>
        <license>
            <name>Plexus</name>
        </license>
    </licenses>
    <scm>
        <url>git@github.com:dom4j/dom4j.git</url>
        <connection>
            <system>git</system>
            <path>git@github.com:dom4j/dom4j.git</path>
        </connection>
        <developerconnection>
            <system>git</system>
            <path>git@github.com:dom4j/dom4j.git</path>
        </developerconnection>
    </scm>
    <repositories>
        <repository>
            <id>xwiki-snapshots</id>
            <type>maven</type>
        </repository>
        <repository>
            <id>xwiki-public-proxy</id>
            <type>maven</type>
        </repository>
        <repository>
            <id>xwiki-public</id>
            <type>maven</type>
        </repository>
    </repositories>
    <properties>
        <maven.groupid>org.dom4j</maven.groupid>
        <maven.artifactid>dom4j</maven.artifactid>
    </properties>
</extension>
